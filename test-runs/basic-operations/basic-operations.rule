REQUIRE test:numbers-one:0.0.1 AS table0;
REQUIRE test:numbers-two:0.0.1 AS table1;

ASSEMBLE add COLUMNS FROM table:numbers;
ASSEMBLE subtract COLUMNS FROM table:numbers;
ASSEMBLE multiply COLUMNS FROM table:numbers;
ASSEMBLE divide COLUMNS FROM table:numbers;

MAP table:add
  USING c = add(@a, @b);

MAP table:subtract
  USING c = subtract(@a, @b);

MAP table:multiply
  USING c = multiply(@a, @b);

MAP table:divide
  USING c = divide(@a, @b);
