REQUIRE test:numbers-one:0.0.1 AS tableone;
REQUIRE test:numbers-two:0.0.1 AS tabletwo;

ASSEMBLE add COLUMNS FROM table:tableone;
ASSEMBLE subtract COLUMNS FROM table:tableone;
ASSEMBLE multiply COLUMNS FROM table:tableone;
ASSEMBLE divide COLUMNS FROM table:tableone;

MAP table:add
  USING c = add(@a, @b);

MAP table:subtract
  USING c = subtract(@a, @b);

MAP table:multiply
  USING c = multiply(@a, @b);

MAP table:divide
  USING c = divide(@a, @b);
